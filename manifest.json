{
    "name": "weibo optimization",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "为了不让微博的PM喂我吃屎而做的优化工具",
    "icons": {
        "16": "image/icon16.png",
        "32": "image/icon32.png"
    },
    "browser_action": {
        "default_title": "微博优化器",
        "default_icon": "image/icon32.png"
    },
    "author": "huaxiaolong",
    "content_scripts": [
        {
          "js": ["script/lib/jquery-1.11.3.min.js", "script/index.js"],
          "matches": [
            "http://weibo.com/*",
            "https://weibo.com/*"
          ],
          "run_at": "document_end"
        }
    ],
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
